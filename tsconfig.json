{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"lib": ["dom", "es6"],
		"module": "ESNext",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "ES6"
	},
	"exclude": ["node_modules"],
	"include": ["src/**/*"]
}
